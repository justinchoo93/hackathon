{
  "manifest_version": 2,
  "name": "Memin",
  "version": "1.0",
  "description": "You got meme?",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["./main.js"]
    }
  ],
  "browser_action": {
    "default_icon": "matrix.jpg",
    "default_popup": "popup.html",
    "default_title": "A popup will come here."
  }
}
